{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "server.js": {
            "maxDuration": 30
        }
    }
}